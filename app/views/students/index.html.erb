<%= form_tag "/students", method: "get" do %>
    <%= label_tag "Enter Student Name:" %><br>
    <%= text_field_tag :query %>
    <%= submit_tag "Search" %>
<% end %>

<ul>
    <% @students.each do |student| %>
        <%= render partial: "student", locals: {student: student} %><br>
    <% end %>
</ul>

